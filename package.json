{
  "name": "react-task-tracker",
  "version": "1.2.0",
  "description": "Task tracker made with reactjs",
  "main": "index.js",
  "author": {
    "name": "Matthew Kincaid",
    "email": "matthew@kincaid.dev"
  },
  "license": "MIT",
  "keywords": [
    "task tracker",
    "react",
    "json server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mdkincaid/TaskTracker.git"
  },
  "bugs": "https://github.com/mdkincaid/TaskTracker/issues",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:start\" \"npm:server\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "set TZ=UTC && react-scripts test",
    "test-coverage": "react-scripts test --watchAll=false --coverage",
    "server": "json-server --watch db.json --port 5000"
  },
  "dependencies": {
    "json-server": "^0.16.3",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "concurrently": "^6.2.0",
    "react-test-renderer": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
